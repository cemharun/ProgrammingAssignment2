makeMatrix <- function(x = matrix()) {
  i <- NULL
  set1 <- function(y) {
    x <<- y
    i <<- NULL
  }
  get1 <- function() x
  setinverse1 <- function(inverse) i <<- inverse
  getinverse1 <- function() i
  list(set = set1,
       get = get1,
       setinverse = setinverse1,
       getinverse = getinverse1)
}

inv_Solve <- function(x, ...) {
  i <- x$getinverse()
  if (!is.null(i)) {
    message("no data")
    return(i)
  }
  data <- x$get()
  i <- solve(data, ...)
  x$setinverse(i)
  i
}
